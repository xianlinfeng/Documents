{
   "version": "2.0.0",
   "options": {
      "cwd": "${workspaceRoot}/Debug"
   },
   "tasks": [
      {
         "label": "cmake",
         "type": "shell",
         "command": "cmake",
         "args": [
            "..",
            "-DCMAKE_PREFIX_PATH=~/scip_installation/Debug/",
            "-DCMAKE_BUILD_TYPE=Debug",
         ]
      },
      {
         "label": "make",
         "group": {
            "kind": "build",
            "isDefault": true
         },
         "type": "shell",
         "command": "make"
      }
   ]
}